{
    "name": "sensiolabs/ansi-to-html",
    "type": "library",
    "description": "A library to convert a text with ANSI codes to HTML",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabien Potencier",
            "email": "fabien@symfony.com"
        }
    ],
    "require": {
        "php": "^7"
    },
    "require-dev": {
        "phpunit/phpunit": "^6",
        "phpstan/phpstan": "^0.9.2",
        "twig/twig": "^2.12",
        "friendsofphp/php-cs-fixer": "^2.15"
    },
    "suggest": {
        "twig/twig": "Provides nice templating features"
    },
    "autoload": {
        "psr-0": { "SensioLabs\\AnsiConverter": "." }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    },
    "scripts": {
        "cs": "vendor/bin/php-cs-fixer fix . -vvv",
        "sa": "vendor/bin/phpstan analyse --ansi -l 7 --no-progress SensioLabs"
    }
}
